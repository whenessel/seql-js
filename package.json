{
  "name": "@whenessel/seql-js",
  "version": "1.1.0",
  "description": "Semantic Element Query Language JS Implementation",
  "author": "Artem Demidenko <ar.demidenko@gmail.com>",
  "scripts": {
    "types:check": "tsc --noEmit",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "test": "vitest run --project unit --project integration",
    "test:watch": "vitest --project unit --project integration",
    "test:coverage": "vitest run --project unit --project integration --coverage",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:unit:coverage": "vitest run --config vitest.config.unit.ts --coverage",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:coverage": "vitest run --config vitest.config.integration.ts --coverage",
    "test:smoke": "vitest run --config vitest.config.smoke.ts",
    "test:ci": "vitest run --coverage",
    "extension:prepare": "yarn build && mkdir -p extensions/chrome/lib && cp dist/seql-js.umd.cjs extensions/chrome/lib/"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whenessel/seql-js.git"
  },
  "keywords": [],
  "main": "./dist/seql-js.umd.cjs",
  "module": "./dist/seql-js.js",
  "unpkg": "./dist/seql-js.umd.cjs",
  "typings": "./dist/seql-js.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/seql-js.d.ts",
        "default": "./dist/seql-js.js"
      },
      "require": {
        "types": "./dist/seql-js.d.cts",
        "default": "./dist/seql-js.umd.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "license": "MIT",
  "public": true,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/whenessel/seql-js/issues"
  },
  "homepage": "https://github.com/whenessel/seql-js/issues#readme",
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@whenessel/jsdom-extended": "^0.1.2",
    "jsdom": "^27.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.22",
  "publishConfig": {
    "access": "public"
  }
}
